<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="callBackendServiceProxy"
       transports="https http"
       startOnLoad="true"
       trace="disable">
   <description/>
   <target>
      <endpoint>
         <address uri="http://localhost:9000/services/BackendService"/>
      </endpoint>
      <inSequence>
         <class name="org.wso2.sample.mediator.MessagePreProcessor">
            <property name="separator" value="|"/>
         </class>
      </inSequence>
      <outSequence>
         <class name="org.wso2.sample.mediator.MessagePostProcessor">
            <property name="splitLength" value="3"/>
         </class>
         <send/>
      </outSequence>
   </target>
</proxy>
