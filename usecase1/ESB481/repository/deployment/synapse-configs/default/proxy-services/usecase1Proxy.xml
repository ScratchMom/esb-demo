<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="usecase1Proxy"
       transports="https"
       startOnLoad="true"
       trace="disable">
   <description/>
   <target>
      <endpoint>
         <address uri="http://localhost:8280/services/ProcessMessageMainProxy"/>
      </endpoint>
      <inSequence/>
      <outSequence>
         <send/>
      </outSequence>
   </target>
   <policy key="conf:/repository/axis2/service-groups/usecase1Proxy/services/usecase1Proxy/policies/policyFromRegistry"/>
   <parameter name="disableREST">true</parameter>
   <parameter name="ScenarioID">policyFromRegistry</parameter>
   <parameter name="secPolicyRegistryPath">conf:/policy/UTOverTransport.xml</parameter>
   <enableSec/>
</proxy>
