<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="PoxSecurityProxy"
       transports="https"
       startOnLoad="true"
       trace="disable">
   <target>
      <endpoint>
         <address uri="http://localhost:8280/services/echo"/>
      </endpoint>
      <outSequence>
         <send/>
      </outSequence>
   </target>
   <publishWSDL uri="http://localhost:8280/services/echo?wsdl"/>
   <policy key="conf:/repository/axis2/service-groups/PoxSecurityProxy/services/PoxSecurityProxy/policies/UTOverTransport"/>
   <parameter name="ScenarioID">scenario1</parameter>
   <enableSec/>
</proxy>
